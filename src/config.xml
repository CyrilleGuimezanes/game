<?xml version="1.0" encoding="utf-8"?><widget id="castle.defense" version="0.0.1" versionCode="0001" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:android="http://http://schemas.android.com/apk/res/android">
  <name>Castle defense</name>
  <description>
    Jeu en dev
    </description>
  <author email="contact@luditeam.com" href="https://luditeam.com">
        Luditeam
    </author>
  <content src="index.html"/>
  <preference name="permissions" value="none"/>
  <preference name="phonegap-version" value="cli-5.2.0"/><!--3.7.0-->
  <preference name="deployment-target" value="6.0"/>
  <preference name="Orientation" value="portrait"/>
  <preference name="target-device" value="universal"/>
  <preference name="Fullscreen" value="True"/>
  <preference name="ShowTitle" value="False"/>
  <preference name="InAppBrowserStorageEnabled" value="False"/>
  <preference name="SuppressesIncrementalRendering" value="True"/>
  <preference name="webviewbounce" value="true"/>
  <preference name="prerendered-icon" value="true"/>
  <preference name="stay-in-webview" value="false"/>
  <preference name="ios-statusbarstyle" value="black-opaque"/>
  <preference name="detect-data-types" value="true"/>
  <preference name="exit-on-suspend" value="false"/>
  <preference name="KeepRunning" value="true"/>
  <preference name="show-splash-screen-spinner" value="true"/>
  <preference name="auto-hide-splash-screen" value="false"/>
  <preference name="disable-cursor" value="false"/>
  <preference name="android-minSdkVersion" value="19"/>
  <preference name="android-installLocation" value="auto"/>
  <preference name="SplashScreen" value="splash"/>
  <preference name="SplashScreenDelay" value="200000"/>
  <preference name="DisallowOverscroll" value="false"/>
  <preference name="android-targetSdkVersion" value="19"/>

  <preference name="android-build-tool" value="gradle"/>
  <preference name="StatusBarOverlaysWebView" value="false"/>
  <preference name="StatusBarBackgroundColor" value="#000000"/>
  <preference name="StatusBarStyle" value="lightcontent"/>
  <gap:plugin name="com.phonegap.plugin.statusbar"/>
  <gap:plugin name="cordova-plugin-device" source="npm"/>
  <gap:plugin name="cordova-plugin-vibration" source="npm"/>
  <gap:plugin name="cordova-plugin-splashscreen" source="npm"/>
  <gap:plugin name="cordova-plugin-backbutton" source="npm"/>

  <gap:plugin name="com.adobe.plugins.gaplugin"/>
  <gap:plugin name="cordova-plugin-network-information" source="npm"/>
  <gap:plugin name="cordova-plugin-globalization" source="npm"/>
  <gap:plugin name="cordova-plugin-media" source="npm"/>
  <gap:plugin name="cordova-plugin-screen-orientation" source="npm"/>
  <!--<gap:plugin name="org.li8.inappbrowser" version="0.1" />-->

  <gap:plugin name="lt.pgb.plugin.adcolony" version="1.0.17"/>
  <!--<gap:plugin name="lt.pgb.plugin.chartboost" version="1.0.17"/>-->

  <!--<gap:plugin name="cordova-plugin-facebook4" source="npm">

    <param name="APP_ID" value="608595505935483"/>
    <param name="APP_NAME" value="Bargain Adventures"/>
  </gap:plugin>-->

  <preference name="permissions" value="none"/>

  <icon src="images/icon.png"/>
  <icon gap:platform="android" gap:qualifier="ldpi" src="images/icon.png"/>
  <icon gap:platform="android" gap:qualifier="mdpi" src="images/icon.png"/>
  <icon gap:platform="android" gap:qualifier="hdpi" src="images/icon.png"/>
  <icon gap:platform="android" gap:qualifier="xhdpi" src="images/icon.png"/>
  <icon gap:platform="blackberry" src="images/icon.png"/>
  <icon gap:platform="blackberry" gap:state="hover" src="images/icon.png"/>
  <icon gap:platform="ios" height="57" src="images/icon/57_icon.png" width="57"/>
  <icon gap:platform="ios" height="72" src="images/icon/72_icon.png" width="72"/>
  <icon gap:platform="ios" height="114" src="images/icon/114_icon.png" width="114"/>
  <icon gap:platform="ios" height="144" src="images/icon/144_icon.png" width="144"/>
  <icon gap:platform="ios" height="76" src="images/icon/76_icon.png" width="76"/>
  <icon gap:platform="ios" height="152" src="images/icon/152_icon.png" width="152"/>
  <icon gap:platform="ios" height="120" src="images/icon/120_icon.png" width="120"/>
  <icon gap:platform="webos" src="images/icon.png"/>
  <icon gap:platform="winphone" src="images/icon.png"/>
  <icon gap:platform="winphone" gap:role="background" src="images/icon.png"/>
  <gap:splash src="images/splash/default-portrait.png"/>
  <gap:splash src="images/splash/ldpi.png" gap:platform="android" gap:qualifier="ldpi"/>
  <gap:splash src="images/splash/mdpi.png" gap:platform="android" gap:qualifier="mdpi"/>
  <gap:splash src="images/splash/hdpi.png" gap:platform="android" gap:qualifier="hdpi"/>
  <gap:splash src="images/splash/xhdpi.png" gap:platform="android" gap:qualifier="xhdpi"/>


  <gap:splash src="images/splash/screen-iphone-portrait.png" gap:platform="ios" width="320" height="480"/>
  <gap:splash src="images/splash/screen-iphone-portrait-2x.png" gap:platform="ios" width="640" height="960"/>
  <gap:splash src="images/splash/screen-iphone-portrait-568h-2x.png" gap:platform="ios" width="640" height="1136"/>
  <gap:splash src="images/splash/screen-ipad-portrait.png" gap:platform="ios" width="768" height="1024"/>
  <gap:splash src="images/splash/screen-ipad-landscape.png" gap:platform="ios" width="1024" height="768"/>

  <gap:splash src="images/splash/screen-portrait.jpg" gap:platform="winphone"/>
  <access origin="*"/>
  <engine name="android" spec="^4.1.1"/>
  <plugin name="cordova-plugin-whitelist" source="npm"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
      <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
      <allow-intent href="itms:*"/>
      <allow-intent href="itms-apps:*"/>
  </platform>
  <gap:config-file platform="ios" parent="CFBundleShortVersionString">
    <string>100</string>
  </gap:config-file>
  <gap:config-file platform="android" parent="/manifest" mode="delete">
      <uses-permission android:name="android.permission.RECORD_AUDIO" />
      <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
  </gap:config-file>


</widget>
